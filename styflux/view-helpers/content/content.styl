/**
* Contents Section
* @class Contents
* @module base.css
*/
.row
	*zoom 1
	&:before,&:after
		display table
		content ""
		line-height 0
	&:after
		clear both
.side-left
	background url(routImg+"bg.png") repeat #8376ae
	float left
	height 100%
	padding 0 20px
	position fixed
	top 0
.content
	margin 10px 30px 0 340px
.box
	padding-top 20px
